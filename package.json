{
  "name": "corporate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "next dev",
    "hcm:watch": "hcm 'src/**/*.module.css' -w",
    "dev": "concurrently \"npm:serve\" \"npm:hcm:watch\" -c blue,green",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "biome check .",
    "format": "biome format . --write && biome check . --apply",
    "typecheck": "tsc",
    "ncu:dev": "ncu -u --dep dev",
    "ncu:prod": "ncu -u --dep prod"
  },
  "dependencies": {
    "next": "14.2.1",
    "next-sitemap": "4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@types/node": "20.12.7",
    "@types/react": "18.2.77",
    "@types/react-dom": "18.2.25",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "happy-css-modules": "2.1.2",
    "npm-check-updates": "16.14.18",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@8.14.1+sha256.2df78e65d433d7693b9d3fbdaf431b2d96bb4f96a2ffecd51a50efe16e50a6a8"
}
