{
  "name": "corporate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "next dev",
    "hcm:watch": "hcm 'src/**/*.module.css' -w",
    "dev": "concurrently \"npm:serve\" \"npm:hcm:watch\" -c blue,green",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "biome lint . && biome format .",
    "format": "biome format . --write && biome lint . --apply",
    "typecheck": "tsc",
    "ncu:dev": "ncu -u --dep dev",
    "ncu:prod": "ncu -u --dep prod"
  },
  "dependencies": {
    "next": "14.0.4",
    "next-sitemap": "4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.4.16",
    "concurrently": "8.2.2",
    "happy-css-modules": "2.1.2",
    "npm-check-updates": "16.14.12",
    "postcss": "8.4.33",
    "prettier": "3.1.1",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3"
  },
  "packageManager": "pnpm@8.14.0+sha256.9cebf61abd83f68177b29484da72da9751390eaad46dfc3072d266bfbb1ba7bf"
}
