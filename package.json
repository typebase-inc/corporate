{
  "name": "corporate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "next dev",
    "hcm:watch": "hcm 'src/**/*.module.css' -w",
    "dev": "concurrently \"npm:serve\" \"npm:hcm:watch\" -c blue,green",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "format": "run-s format:prettier format:eslint",
    "format:eslint": "pnpm lint:eslint --fix",
    "format:prettier": "pnpm lint:prettier --write",
    "typecheck": "tsc",
    "ncu:dev": "ncu -u --dep dev",
    "ncu:prod": "ncu -u --dep prod"
  },
  "dependencies": {
    "next": "13.5.6",
    "next-sitemap": "4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.8.7",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "autoprefixer": "10.4.16",
    "concurrently": "8.2.2",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "happy-css-modules": "2.1.2",
    "npm-check-updates": "16.14.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "packageManager": "pnpm@8.9.2+sha256.8d62573d93061f2722b7b48c9739e96cd4603c3ab153bc81c619dcb9861a214e"
}
